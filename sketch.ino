void setup() {
  Serial.begin(9600);
  Serial.println("Hello, ESP32!");
  pinMode(start, INPUT);
  pinMode(sel, INPUT);
  pinMode(ENCODER_CLK, INPUT);
  prev_CLK_state = HIGH;
  pinMode(ENCODER_DT, INPUT);

  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }
  if (!mpu.begin()) {
    Serial.println("Failed to find MPU6050 chip");
    while (1) {
      delay(10);
    }
  }
  mpu.setAccelerometerRange(MPU6050_RANGE_16_G);
  mpu.setGyroRange(MPU6050_RANGE_250_DEG);
  mpu.setFilterBandwidth(MPU6050_BAND_21_HZ);

  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(SSD1306_WHITE);
  beginning();
}

int lastClk = HIGH;
void loop() {

    if(digitalRead(start) == HIGH){
      if(beg && stage == 1){
        men(1);
        delay(5);
      }
      else if(!inMenu){
        openMenu();
      }
    }
    if(digitalRead(sel) == HIGH){
      if(fishing){
        casting = true;
        clearInstructions();
      }
      if(beg){
        men(stage);
      }
      if(inMenu){
        enter(oldPos);
      }
      if(inShop){
        buy(oldPos);
      }
      if(cau){
        cau = false;
        casting = false;
        startFishing();
      }
      delay(5);
    }

    if(inMenu || inShop){
      drawCursor(analogRead(slid), 3);
      delay(5);
    }
    if(fishing && !casting){
      int newClk = digitalRead(ENCODER_CLK);
      if (newClk != prev_CLK_state) {
        prev_CLK_state = newClk;
        int dtValue = digitalRead(ENCODER_DT);
        if (newClk == LOW && dtValue == HIGH) {
          rotatePlayerRight();
        }
        if (newClk == LOW && dtValue == LOW) {
          rotatePlayerLeft();
        }
      }
    }
  
    if(casting){
      sensors_event_t a, g, temp;
      mpu.getEvent(&a, &g, &temp);

      //Magnitude
      double magnitude = sqrt(a.acceleration.x * a.acceleration.x + a.acceleration.y * a.acceleration.y + a.acceleration.z * a.acceleration.z);
      int adjustedMag = map(magnitude, 9.81, 33.97, 0, 100);
      if(adjustedMag > 0){
        cast(adjustedMag);
        casting = false;
        oldReelPos = analogRead(slid);
      }
      delay(100);
    }

    if(reeling){
      int newReelPos = analogRead(slid);
      if(newReelPos != oldReelPos)
        reel(analogRead(slid), oldReelPos);
      oldReelPos = newReelPos;
      delay(5);
    }



}

